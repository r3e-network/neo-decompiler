{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://github.com/r3e-network/neo-decompiler/docs/schema/info.schema.json",
  "title": "neo-decompiler info report",
  "type": "object",
  "required": ["file", "method_tokens", "warnings"],
  "properties": {
    "file": {"type": "string"},
    "manifest_path": {"type": ["string", "null"]},
    "compiler": {"type": ["string", "null"]},
    "source": {"type": ["string", "null"]},
    "script_length": {"type": "integer"},
    "script_hash_le": {"type": "string", "pattern": "^[0-9A-F]+$"},
    "script_hash_be": {"type": "string", "pattern": "^[0-9A-F]+$"},
    "checksum": {"type": "string"},
    "method_tokens": {
      "type": "array",
      "items": {"$ref": "#/definitions/methodToken"}
    },
    "manifest": {"$ref": "#/definitions/manifestSummary"},
    "warnings": {
      "type": "array",
      "items": {"type": "string"}
    }
  },
  "definitions": {
    "manifestSummary": {
      "type": ["object", "null"],
      "properties": {
        "name": {"type": "string"},
        "supported_standards": {
          "type": "array",
          "items": {"type": "string"}
        },
        "storage": {"type": "boolean"},
        "payable": {"type": "boolean"},
        "permissions": {
          "type": "array",
          "items": {"$ref": "#/definitions/permissionSummary"}
        },
        "trusts": {"$ref": "#/definitions/trustSummary"},
        "abi": {
          "type": "object",
          "properties": {
            "methods": {"type": "array"},
            "events": {"type": "array"}
          }
        }
      }
    },
    "permissionSummary": {
      "type": "object",
      "properties": {
        "contract": {
          "type": "object",
          "properties": {"type": {"type": "string"}, "value": {"type": ["string", "null"]}}
        },
        "methods": {
          "type": "object",
          "properties": {"type": {"type": "string"}, "value": {"type": ["array", "string", "null"]}}
        }
      }
    },
    "trustSummary": {
      "type": ["object", "null"],
      "properties": {
        "type": {"type": "string"},
        "value": {"type": ["array", "string", "null"]}
      }
    },
    "methodToken": {
      "type": "object",
      "properties": {
        "method": {"type": "string"},
        "hash_le": {"type": "string"},
        "hash_be": {"type": "string"},
        "parameters": {"type": "integer"},
        "returns": {"type": "boolean"},
        "call_flags": {"type": "integer"},
        "call_flag_labels": {
          "type": "array",
          "items": {"type": "string"}
        },
        "native_contract": {"type": ["object", "null"]},
        "warning": {"type": ["string", "null"]}
      }
    }
  }
}
