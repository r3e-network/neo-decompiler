{
    "name": "MetaEvent",
    "groups": [],
    "features": {
        "storage": true,
        "payable": true
    },
    "supportedstandards": [
        "NEP-11",
        "NEP-17"
    ],
    "abi": {
        "methods": [
            {
                "name": "main",
                "parameters": [
                    {
                        "name": "from",
                        "type": "Hash160"
                    },
                    {
                        "name": "amount",
                        "type": "Integer"
                    }
                ],
                "returntype": "Void",
                "offset": 0,
                "safe": false
            }
        ],
        "events": [
            {
                "name": "Transfer",
                "parameters": [
                    {
                        "name": "from",
                        "type": "Hash160"
                    },
                    {
                        "name": "to",
                        "type": "Hash160"
                    },
                    {
                        "name": "amount",
                        "type": "Integer"
                    }
                ]
            }
        ]
    },
    "permissions": [
        {
            "contract": "*",
            "methods": "*"
        }
    ],
    "trusts": "*",
    "extra": {
        "Author": "Edge Case",
        "Email": "edge@example.com"
    }
}